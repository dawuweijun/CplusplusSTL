    'P12' 2*  1  2  'OPEN' 0   -1   0.5  /
    'P16' 2*  2  3  'OPEN' 0   -1   0.5  /
    'P20' 2*  3  4  'OPEN' 0   -1   0.5  /
    'P24' 2*  4  5  'OPEN' 0   -1   0.5  /
    /

